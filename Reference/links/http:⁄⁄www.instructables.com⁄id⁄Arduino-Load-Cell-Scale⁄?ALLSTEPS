<script>
    window.Ibles = window.Ibles || {};    
    Ibles.reverseUrls =  {
        home: function() {
            return "/";
        },
        memberUrl: function(screenName) {
            return "/member/SCREENNAME/".replace("SCREENNAME", screenName);
        },
        memberFavUrl: function(screenName) {
            return "/member/SCREENNAME/favorites/".replace("SCREENNAME", screenName);
        },
        memberIblesUrl: function(screenName) {
            return "/member/SCREENNAME/instructables/".replace("SCREENNAME", screenName);
        },
        ibleUrl: function(id, allSteps) {
            var url;
            if (allSteps)
                url = "/id/IBLE/?ALLSTEPS";
            else
                url = "/id/IBLE/";
            return url.replace("IBLE", id);
        },
        ibleEditUrl: function(id) {
            return "/editInstructable/edit/IBLE/".replace('IBLE', id);
        },
        previewUrl:function(id) {
            var url = "/preview/IBLE/";
            return url.replace("IBLE", id);
        },
        editorCreate: function() {
            return "/editInstructable/new/";
        },
        collectionEditUrl: function(id) {
            return "/editCollection/COLLECTION_ID/".replace('COLLECTION_ID', id);  
        },
        contestDetailUrl: function(urlString) {
            return "/contest/URLST/".replace('URLST', urlString);
        },
        groupUrl: function(urlString) {
            return "/group/URLST/".replace('URLST', urlString);
        },             
        loginUrl: function() {
            return "/account/login/";
        },
        nativeAppLoginUrl: function() {
            return "/account/nativeapp/login/";
        },        
        signupUrl: function() {
            return "/account/register/";
        },
        nativeAppSignupUrl: function() {
            return "/account/nativeapp/signup/";
        },
        forgotPasswordUrl: function() {
            return "/account/forgot/";
        },
        nativeAppForgotPasswordUrl: function() {
            return "/account/nativeapp/forgot/";
        },        
        resetPasswordUrl: function() {
            return "/account/reset/";
        },
        nativeAppResetPasswordUrl: function() {
            return "/account/nativeapp/reset/";
        },        
        staticUrl: function() {
            return "/intl_static/"
        },
		s3Url:function(path, filename) {
			return "http://cdn.instructables.com/" + path + filename;
		},
        bulkAction: function() {
            return "/tools/bulkaction/";
        },

        // Channels currently only exist in the main site code base
        // so there's no reverse URL function available for them.
        // For category and channel, use the normal name, not the display name.
        channelUrl: function(locale, category, channel) {
            if (locale === 'en_US') {
				
                return "/tag/type-id/category-technology/channel-CHANNEL/".replace('technology', category).replace('CHANNEL', channel);
				
			}
            else {
                return this.exploreUrl(category,channel);
            }
        },
		exploreUrl:function(category, channel){
			if (_.isUndefined(channel)){
				return "/explore/".replace('explore',category)
			} else {
				return "/explore/CHANNEL/".replace('explore',category).replace('CHANNEL',channel);
			}
		},
		searchUrl: function(searchTerm) {
			return "/howto/QUERYSTRING/".replace('QUERYSTRING', searchTerm);
		},
		analyticsApi: function(method) {
			return "/tools/analytics/api/method/".replace('method', method);
		}
	};
</script>



    
